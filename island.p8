pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
function _init()
	p1 = {}
	p1.x = 32
	p1.y = 64
	p1.offset = 0
	p1.offtime_max = 10
	p1.offtime = 0
	p1.left = false
end

function _update60()

	p1.offtime = (p1.offtime+1)%p1.offtime_max
	if (p1.offtime == p1.offtime_max-1) p1.offset = 1-p1.offset
	
	p1.y += 1
	if fget(mget(p1.x\8,(p1.y+8)\8),0) then
		p1.y = 8*(p1.y\8)
	end
	
	p1.drawx = p1.x-4
	p1.drawy = p1.y-16
	p1.sp = 1+2*p1.offset
	
	if btn(0) then
		p1.x += -1
		p1.left = true
		p1.offtime += 1
		if (p1.offtime == p1.offtime_max-1) p1.offset = 1-p1.offset
	end
	
	if btn(1) then
		p1.x += 1
		p1.left = false
		p1.offtime += 1
		if (p1.offtime == p1.offtime_max-1) p1.offset = 1-p1.offset
	end	
end

function _draw()
	cls(12)
	palt(0,false)
	palt(13,true)
	map()
	spr(p1.sp,p1.drawx,p1.drawy,2,3,p1.left)
	palt()
end
